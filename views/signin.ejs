<html>

<head>
  <title>Log-In</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<nav class="navbar navbar-dark bg-dark">
  <a class="navbar-brand" href="/">Whats-App</a>
  <a class="btn btn-primary" href="/signup"> Sign-Up </a>
</nav>

<body>
  <div class="container">
    <div id="wrapper">
    </div>
    <div class="card text-center">
      <h5 class="card-header">Log-In</h5>
      <div class="card-body">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSEy0RnM81qo5Es2l1WQhFbOtV4hsbdb0FR4w&usqp=CAU"
          height="134" width="134"><br><br>
        <form action="/signin" method="post">
          <div class="input-group">
            <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i
                  class="fas fa-at"></i></span></div>
            <input class="form-control" type="email" onfocus="this.value=''" id="email" placeholder="E-mail" required
              maxlength="30">
            <span class="addBtn"></span>
          </div><br>
          <div class="input-group">
            <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i
                  class="fas fa-lock"></i></span></div>
            <input class="form-control" type="password" onfocus="this.value=''" id="password" placeholder="Password"
              required maxlength="30">
            <span class="addBtn"></span>
          </div><br>
          <button onclick="logIn()" action="logIn" class="btn btn-primary" type="submit" form="nameform" value="Submit" style="width:100%;"> LogIn  <i class="fa fa-sign-in"></i>
          </button>
        </form>
        <a href="/forget" style="float:right;"> Forget-Password  <i class="fas fa-lock"></i>
        </button>

      </div>
    </div><br>
  </div>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="../js/signin.js"></script>
</body>

</html>